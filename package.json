{
  "name": "EduAnat2",
  "productName": "EduAnat2",
  "description": "EduAnat2",
  "author": "Remi AGIER <remi@aigen.fr> (http://www.aigen.fr), Sebastien Valette <sebastien.valette@creatis.insa-lyon.fr> (http://www.creatis.insa-lyon.fr/~valette)",
  "license": "CECILL-B",
  "version": "2.2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/valette/eduAnat2.git"
  },
  "main": "script.js",
  "dependencies": {},
  "devDependencies": {
    "@qooxdoo/compiler": "1.0.0",
    "acorn": "8.2.4",
    "async": "3.2.0",
    "bluebird": "3.7.2",
    "bowser": "2.11.0",
    "buffer": "6.0.3",
    "chroma-js": "2.1.2",
    "crypto-browserify": "3.12.0",
    "css-loader": "5.2.6",
    "desk-ui": "valette/desk-ui",
    "electron": "13.0.1",
    "electron-builder": "22.10.5",
    "electron-packager": "15.2.0",
    "jssha": "3.2.0",
    "lodash": "4.17.21",
    "numeric": "1.2.6",
    "papaya-viewer": "1.0.1449",
    "random-js": "2.1.0",
    "script-loader": "0.7.2",
    "socket.io-client": "4.1.2",
    "source-map-loader": "3.0.0",
    "stream-browserify": "3.0.0",
    "style-loader": "2.0.0",
    "three": "0.128.0",
    "url-loader": "4.1.1",
    "webpack": "5.37.1",
    "webpack-cli": "4.7.0",
    "webworkify-webpack": "2.1.5",
    "xterm": "4.12.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run buildApps && npm run package",
    "buildApps": "npm run buildCore && cp script.js compiled/dist",
    "buildCore": "node node_modules/desk-ui/source/build-worker.js && webpack && npm run qooxdoo",
    "clean": "rm -rf compiled && rm -rf dist && rm -rf EduAnat2-*",
    "compile": "qx compile --target source && qx compile --target build",
    "setDate": "date > source/resource/eduAnat2/buildDate.txt",
    "deploy": "qx deploy --clean --out compiled/dist",
    "dev": "qx compile --target source --watch --update-po-files",
    "package": "npm run package-bin && npm run package-exe-deb-rpm && npm run zip",
    "package-bin": "electron-packager --all --overwrite --prune=true --icon=icon.ico --azar=true compiled/dist/ EduAnat2",
    "package-exe-deb-rpm": "electron-builder build -wl --config builder-effective-config.yaml",
    "qooxdoo": "npm run setDate && qx package update && qx package install && npm run compile && npm run deploy && ./extra.sh",
    "start": "electron .",
    "updateAll": "rm -rf node_modules && git pull && npm install && npm run build",
    "zip": "for i in ./EduAnat2-*; do zip -rqy \"dist/${i%/}.zip\" \"$i\"; done"
  }
}
